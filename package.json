{
	"name": "paste-json-as-reactive-form-builder",
	"displayName": "paste json as reactive form builder",
	"description": "paste json as angular rective form builder",
	"version": "0.0.1",
	"icon": "icon.png",
	"author": {
        "name": "jawahar gopal",
        "email": "gjawahargopal@gmail.com"
    },
	"engines": {
		"vscode": "^1.30.0"
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/jawahargopal/paste-json-as-angular-reactive-form"
	},
	"bugs": {
        "url": "https://github.com/jawahargopal/paste-json-as-angular-reactive-form/issues"
    },
	"categories": [
