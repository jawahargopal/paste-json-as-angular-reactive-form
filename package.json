{
	"name": "paste-json-as-reactive-form-builder",
	"displayName": "paste json as reactive form builder",
	"description": "paste json as angular rective form builder",
	"version": "0.0.1",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.pasteJSONAsReactiveForm"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.pasteJSONAsReactiveForm",
				"title": "Paste JSON as Reactive Form"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/clipboardy": "^1.1.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"clipboardy": "^1.2.3"
	}
}
